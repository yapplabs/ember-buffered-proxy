{
  "name": "ember-buffered-proxy",
  "version": "2.0.0",
  "description": "An Ember Object Proxy with change buffering",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/yapplabs/ember-buffered-proxy",
  "license": "MIT",
  "author": "Kris Selden, Stefan Penner & Yapp Labs",
  "contributors": [
    {
      "name": "Ben Limmer",
      "email": "hello@benlimmer.com"
    },
    {
      "name": "Tomek NieÅ¼urawski",
      "email": "tommaqs@gmail.com"
    }
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "release": "release-it",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-compatibility-helpers": "^1.2.6",
    "ember-notify-property-change-polyfill": "^0.0.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.1.4",
    "@glimmer/component": "^1.0.3",
    "@glimmer/tracking": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.10.1",
    "ember-cli": "~3.24.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-release": "^1.0.0-beta.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.1",
    "ember-cli-update": "^0.45.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-page-title": "^6.0.3",
    "ember-qunit": "^5.1.1",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.24.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.15.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-ember": "^10.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "qunit": "^2.13.0",
    "qunit-dom": "^1.6.0",
    "release-it": "^14.2.1",
    "release-it-lerna-changelog": "^3.1.0"
  },
  "engines": {
    "node": "12.* || >= 14"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": false
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "volta": {
    "node": "14.17.4",
    "yarn": "1.22.11"
  }
}
